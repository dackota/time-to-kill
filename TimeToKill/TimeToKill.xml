<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..">
  <Script file="TimeToKill.lua"/>
  <Frame name="TimeToKillFrame" inherits="BackdropTemplate" hidden="false" movable="true" enableMouse="true" resizable="true">
    <Size>
      <AbsDimension x="200" y="50"/>
    </Size>
    <ResizeBounds>
      <minResize>
        <AbsDimension x="150" y="30"/>
      </minResize>
      <maxResize>
        <AbsDimension x="400" y="100"/>
      </maxResize>
    </ResizeBounds>
    <Anchors>
      <Anchor point="CENTER"/>
    </Anchors>
    <Layers>
      <Layer level="ARTWORK">
        <FontString name="$parentText" inherits="GameFontNormal" justifyH="CENTER" justifyV="MIDDLE">
          <Anchors>
            <Anchor point="CENTER"/>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
  </Frame>
</Ui>
